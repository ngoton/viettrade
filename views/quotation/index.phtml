   });

$('#loc_from_name').on('keydown', function() {
        var key = event.keyCode || event.charCode;

        if( key == 8 || key == 46 ){
            $('#loc_from_name').attr('data',"");
        }
            
      }); 
$('#loc_to_name').on('keydown', function() {
        var key = event.keyCode || event.charCode;

        if( key == 8 || key == 46 ){
            $('#loc_to_name').attr('data',"");
        }
            
      }); 

function set_item_location_from(value,name) {
    // change input value
    $('#loc_from_name').val(name);
    $("#loc_from_name").attr("data",value);
    
    // hide proposition list
    $('#loc_from_list_id').hide();
    $('#loc_from_name').focus();

    $("#export_from").val(value);
    
}
function set_item_location_to(value,name) {
    // change input value
    $('#loc_to_name').val(name);
    $("#loc_to_name").attr("data",value);
    
    // hide proposition list
    $('#loc_to_list_id').hide();
    $('#loc_to_name').focus();

    $("#export_to").val(value);
}

