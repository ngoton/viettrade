$('#loc_from_name').keyup(function(){
    $("#loc_from_name").attr("data","");
        var dist = $('#loc_from').val();
        var keyword = $(this).val();
        $.ajax({
            url: '<?php echo BASE_URL ?>/quotation/getlocationfrom',
            type: 'POST',
            data: {keyword:keyword,district:dist},
            success:function(data){
                if (data.trim() == "") {
                    $('#loc_from_list_id').slideUp(200);
                }
                else{
                    $('#loc_from_list_id').slideDown(200);
                    $('#loc_from_list_id').html(data);
                }
            }
        });
      
   });
